{
	"usesTypeScript": false,
	"usesCssModule": true,
	"cssPreprocessor": "scss",
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"customTemplates": {
				"component": "templates/default/TemplateName.jsx",
				"style": "templates/default/TemplateName.module.scss",
				"index": "templates/default/index.js"
			},
			"path": "src/components",
			"withLazy": false,
			"withStory": false,
			"withStyle": true,
			"withTest": true,
			"withIndex": true
		},
		"page": {
			"customTemplates": {
				"component": "templates/page/TemplateName.jsx",
				"style": "templates/page/TemplateName.module.scss",
				"index": "templates/page/index.js"
			},
			"path": "src/pages",
			"withLazy": false,
			"withStory": false,
			"withStyle": true,
			"withTest": true,
			"withIndex": true
		},
		"layout": {
			"path": "src/layout",
			"withLazy": false,
			"withStory": false,
			"withStyle": false,
			"withTest": true
		}
	}
}